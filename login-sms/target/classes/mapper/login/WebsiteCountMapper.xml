<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyc.login.mapper.WebsiteCountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cqyc.login.domain.WebsiteCount">
        <id column="id" property="id" />
        <result column="user_register_count" property="userRegisterCount" />
        <result column="website_look" property="websiteLook" />
        <result column="website_vehicle_appoint" property="websiteVehicleAppoint" />
        <result column="website_liked_count" property="websiteLikedCount" />
        <result column="website_month" property="websiteMonth" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_register_count, website_look, website_vehicle_appoint, website_liked_count, website_month
    </sql>

</mapper>
