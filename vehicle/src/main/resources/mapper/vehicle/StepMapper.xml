<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyc.vehicle.mapper.StepMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cqyc.vehicle.domain.Step">
        <id column="id" property="id" />
        <result column="step_title" property="stepTitle" />
        <result column="step_indicator" property="stepIndicator" />
        <result column="step_introduce" property="stepIntroduce" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, step_title, step_indicator, step_introduce
    </sql>

</mapper>
