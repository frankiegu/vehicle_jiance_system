<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyc.vehicle.mapper.ResultMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cqyc.vehicle.domain.Result">
        <id column="id" property="id" />
        <result column="result_time" property="resultTime" />
        <result column="result_appoint_id" property="resultAppointId" />
        <result column="result_content" property="resultContent" />
        <result column="result_user_id" property="resultUserId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, result_time, result_appoint_id, result_content, result_user_id
    </sql>

</mapper>
